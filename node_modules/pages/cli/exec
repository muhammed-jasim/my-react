#!/usr/bin/env node

fs = require('fs')
path = require('path');

global.pkgPath = path.join(path.dirname(fs.realpathSync(__filename)));
global.pkgBasePath = path.join(path.dirname(fs.realpathSync(__filename))).replace('/cli', '');

require("" + global.pkgBasePath + "/node_modules/coffee-script"); 
require('./');